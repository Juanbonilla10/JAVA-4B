<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/contactenos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>iri</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>irs</from-outcome>
            <to-view-id>/serviciossinuser.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>irn</from-outcome>
            <to-view-id>/nosotros.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>irm</from-outcome>
            <to-view-id>/mision.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
</faces-config>
